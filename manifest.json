{
    "name": "File ZIPper",
    "version": "1.0",
    "description": "Takes files, folders, etc. and compresses them in a ZIP file.",
    "browser_action": {
        "default_icon": "zip16.png"
        },
    "file_browser_handlers": [{
        "id": "zip",
        "default_title": "ZIP files",
        "file_filters": [
            "filesystem:*.*"
            ]
        }],
    "permissions" : [
        "fileBrowserHandler"
        ],
    "icons": {
        "16": "zip16.png",
        "48": "zip48.png",
        "128": "zip128.png"
        },
}